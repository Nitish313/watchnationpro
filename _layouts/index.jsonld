{
  "@context": "http://schema.org",
  "@type": "WebSite",
  "name": "{{ site.title }}",
  "url": "{{ site.url }}",
  "description": "{{ site.description }}",
  "dateModified": "{{ site.time | date_to_xmlschema }}",
  "author": {
    "@type": "Person",
    "name": {{ site.author_name | jsonify }},
   }
}
